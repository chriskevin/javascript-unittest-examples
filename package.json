{
  "name": "javascript-unittest-examples",
  "version": "2.0.0",
  "description": "Examples of Javascript unit testing",
  "main": " ",
  "directories": {
    "src": "app",
    "test": "app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chriskevin/javascript-unittest-examples.git"
  },
  "keywords": [
    "javascript",
    "unit test",
    "jasmine",
    "angularjs"
  ],
  "author": "Chris Sundberg",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/chriskevin/javascript-unittest-examples/issues"
  },
  "homepage": "https://github.com/chriskevin/javascript-unittest-examples",
  "engines": {
    "node": "6.0.0"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-mocks": "1.5.8",
    "react": "15.3.2"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-preset-es2015": "6.16.0",
    "eslint": "3.8.1",
    "eslint-config-google": "0.6.0",
    "eslint-plugin-angular": "1.4.1",
    "eslint-plugin-jasmine": "1.8.1",
    "eslint-plugin-json": "1.2.0",
    "faucet": "0.0.1",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-tap": "3.1.1",
    "karma-tap-pretty-reporter": "3.0.4",
    "karma-webpack": "1.8.0",
    "npm-run-all": "3.1.1",
    "plato": "1.7.0",
    "pre-git": "3.10.0",
    "rimraf": "2.5.4",
    "tape": "4.6.2",
    "updtr": "0.2.1",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2",
    "yarn": "0.15.1"
  },
  "config": {
    "pre-git": {
      "pre-commit": [

      ],
      "pre-push": [
        "npm test"
      ],
      "post-commit": [

      ],
      "post-checkout": [
        "yarn"
      ],
      "post-merge": [
        "yarn"
      ]
    }
  },
  "scripts": {
    "prebuild": "npm test",
    "build": "NODE_ENV=production webpack",
    "postbuild": "cp -rf build/*.js dist",
    "clean": "rimraf build/*.js coverage report",
    "commit": "commit-wizard",
    "lint:js": "eslint . --ext .js --ext .json --ignore-path .gitignore",
    "start": "webpack-dev-server",
    "stats:js": "plato -r -d report -t \"Code metrics\" -e .eslintrc app",
    "test": "run-s lint:js test:karma",
    "test:karma": "karma start config/karma.config.js",
    "upgrade": "updtr --save-exact"
  }
}
